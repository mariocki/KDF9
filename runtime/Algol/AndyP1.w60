    'real' a, y, z;

    open(20);
    a := read(20);
    y := read(20);
    close(20);

    z := (a + y);
    z := (a - y);
    z := (a / y);
    z := (a % y);
    z := (a * y);
    z := (a ~ y);

    'comment' open(30);
    writetext(30, {{z}number_of_primes_below_2^12:_});
    'comment' close(30);
'end'
