# rtl : Labels tapes
#------------------------------------------------------------------------

if [ -z $KDF9ROOT ]
then
    echo "Please set the KDF9ROOT environment variable to the path of ee9.";
    exit 1;
fi

BINARY=${KDF9ROOT}/Binary

>${KDF9ROOT}/MT0
>${KDF9ROOT}/MT1
>${KDF9ROOT}/MT2
>${KDF9ROOT}/MT3
>${KDF9ROOT}/MT4
>${KDF9ROOT}/MT5
>${KDF9ROOT}/ST0
${KDF9ROOT}/nine_test ${BINARY}/RLT ${1:-RLT_data} $2 $3 $4
