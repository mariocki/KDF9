'begin'
  'comment' Knuth's Man or Boy program;

   'procedure' output(dv, i);
   'value' dv, i;
   'integer' dv, i;
   'KDF9' 1/0/0/0;
      {dv}; {i}; MRWDQ0; 'EXIT';
   'ALGOL';

  'real' 'procedure' A(k, x1, x2, x3, x4, x5);
  'value' k; 'integer' k, x1, x2, x3, x4, x5;
  'begin'
    'real' 'procedure' B;
    'begin' k := k - 1;
          B := A := A(k, B, x1, x2, x3, x4);
    'end';
    'if' k <= 0 'then' A := x4 + x5 'else' B;
  'end';
  output(30, A(10, 1, -1, -1, 1, 0));
'end'
