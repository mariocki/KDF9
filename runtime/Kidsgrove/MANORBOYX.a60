!begin
!library A0, A1, A4, A5, A12, A15;
   !integer i, layout;

   !integer !procedure A (k, x1, x2, x3, x4, x5);
      !value k;
      !integer k, x1, x2, x3, x4, x5;
   !begin

       !integer !procedure B;
       !begin
          k := k - 1;
          A := B := A (k, B, x1, x2, x3, x4);
       !end B;

      !if k <= 0 !then A := x4 + x5 !else B;

   !end A;

   open(30);
   layout := format({+nnnd});

   !for i := 0 !step 1 !until 10 !do
