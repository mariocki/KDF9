USERCODE=${KDF9_USERCODE:-Assembly}

prog=$1

if [ ! -r ${USERCODE}/$1.k3 ]
then
   echo The Usercode file "$1" is absent or unreadable.
   echo
   exit 2
fi

extract_symbols $prog
if [ -r settings_3.txt ]
then
   cp settings_3.txt settings_1.txt
fi
cat settings_symbols.txt >> settings_1.txt

. nine $*

