KDF9ROOT=${KDF9RUNTIME}

if [ -z $KDF9ROOT ]
then
    KDF9ROOT="~/.kdf9/"
fi

KDF9BIN=${KDF9ROOT}

${KDF9BIN}/nine "$1" "$2" "$3" "$4" "$5" "$6" t
