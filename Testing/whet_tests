trap "rm -f whet_tests_diff.txt"  \
      0 1 2 3

>whet_tests_log.txt

whet FLUID     t prh >>whet_tests_log.txt
fgrep "Digital signature" KDF9_log.txt >>whet_tests_log.txt
echo FLUID DONE      >>whet_tests_log.txt

whet GPSPRIMES t prh >>whet_tests_log.txt
fgrep "Digital signature" KDF9_log.txt >>whet_tests_log.txt
echo GPSPRIMES DONE  >>whet_tests_log.txt

whet HANOI     t prh >>whet_tests_log.txt
fgrep "Digital signature" KDF9_log.txt >>whet_tests_log.txt
echo HANOI DONE      >>whet_tests_log.txt

whet INPUT     t prh >>whet_tests_log.txt
fgrep "Digital signature" KDF9_log.txt >>whet_tests_log.txt
echo INPUT DONE      >>whet_tests_log.txt

whet MANORBOY  t prh >>whet_tests_log.txt
fgrep "Digital signature" KDF9_log.txt >>whet_tests_log.txt
echo MANORBOY DONE   >>whet_tests_log.txt

whet MATMULT   t prh >>whet_tests_log.txt
fgrep "Digital signature" KDF9_log.txt >>whet_tests_log.txt
echo MATMULT DONE    >>whet_tests_log.txt

whet PENTOMINO t prh >>whet_tests_log.txt
fgrep "Digital signature" KDF9_log.txt >>whet_tests_log.txt
echo TP1:  >>whet_tests_log.txt
echo ===   >>whet_tests_log.txt
cat TP1    >>whet_tests_log.txt
echo ===   >>whet_tests_log.txt
echo PENTOMINO DONE  >>whet_tests_log.txt

whet WEEHAAVIE t prh >>whet_tests_log.txt
fgrep "Digital signature" KDF9_log.txt >>whet_tests_log.txt
echo WEEHAAVIE DONE  >>whet_tests_log.txt

whet WHETSTONE t prh >>whet_tests_log.txt
fgrep "Digital signature" KDF9_log.txt >>whet_tests_log.txt
echo WHETSTONE DONE  >>whet_tests_log.txt

whet ZELLER    t prh >>whet_tests_log.txt
fgrep "Digital signature" KDF9_log.txt >>whet_tests_log.txt
echo ZELLER DONE     >>whet_tests_log.txt

echo
diff whet_tests_log.txt whet_tests_good_log.txt >whet_tests_diff.txt
if [ -s whet_tests_diff.txt ]
then
   echo These are the unexpected variances for this run:
   echo ===
   more whet_tests_diff.txt
   echo ===
else
   echo These tests ran as expected.
fi
