> KDF9_log.txt
this_test="$2"
echo "+++++++++++++++++++ BEGIN  Test Case $1 +++++++++++++++++++"
if /bin/sh $this_test
then
   case x$3x in
   xx)  exit_code=0
        ;;
   x*x) echo The signature of this test should be "$3". It was as follows:
        fgrep "Digital signature of traced orders = " KDF9_log.txt
        echo "------------------- END OF Test Case $1 -------------------"
        exit_code=0
        ;;
   esac
else
   echo !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
   echo Test Case "$1": $this_test FAILED!
   echo "------------------- END OF Test Case $1 -------------------"
   exit_code=1
fi
echo
echo
exit $exit_code

